# Go API port
PORT=8082

# ZITADEL OAuth2 introspection endpoint
ZITADEL_INTROSPECTION_URL=http://localhost:8080/oauth/v2/introspect

# Confidential client credentials used by the API to introspect tokens
ZITADEL_CLIENT_ID=
ZITADEL_CLIENT_SECRET=

# Optional: enforce audience in token
ZITADEL_AUDIENCE=
